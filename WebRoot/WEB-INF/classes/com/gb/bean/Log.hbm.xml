<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gb.bean.Log" table="log">
		<id name="logid" type="java.lang.Integer" column="logid" length="4">
			<generator class="increment"></generator>
		</id>
		<property name="log" type="string" column="log" length="255">
		</property>
		<property name="address" type="string" column="address"
			length="255">
		</property>
		<property name="tjtime" type="string" column="tjtime" length="100">
		</property>


	</class>
</hibernate-mapping>