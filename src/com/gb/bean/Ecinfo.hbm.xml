<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gb.bean.Ecinfo" table="ecinfo">
		<id name="id" type="java.lang.Integer" column="id" length="4">
			<generator class="identity"></generator>
		</id>
		<property name="OprCode" type="string" column="OprCode"
			length="255">
		</property>
		<property name="ECID" type="string" column="ECID" length="255">
		</property>
		<property name="ProvECID" type="string" column="ProvECID"
			length="255">
		</property>
		<property name="ECName" type="string" column="ECName" length="255">
		</property>
		<property name="ECEName" type="string" column="ECEName"
			length="255">
		</property>
		<property name="OprTime" type="string" column="OprTime"
			length="255">
		</property>
		<property name="EffTime" type="string" column="EffTime"
			length="255">
		</property>

		<property name="ECTel" type="string" column="ECTel" length="255">
		</property>

		<property name="IntroURL" type="string" column="IntroURL"
			length="255">
		</property>

		<property name="ContractTel" type="string" column="ContractTel"
			length="255">
		</property>

		<property name="ContractName" type="string" column="ContractName"
			length="255">
		</property>

		<property name="Note" type="string" column="Note" length="255">
		</property>

		<property name="tjtime" type="string" column="tjtime" length="100">
		</property>

 		
	</class>
	
</hibernate-mapping>